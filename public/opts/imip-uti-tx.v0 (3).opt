                    <items id="description">Any timed recording of Body Mass Index.</items>
                </term_definitions>
                <term_definitions code="at0003">
                    <items id="text">Single</items>
                    <items id="description">@ internal @</items>
                </term_definitions>
                <term_definitions code="at0004">
                    <items id="text">Relative eGFR</items>
                    <items id="description">Estimation of glomerular filtration rate normalised to a body surface area of 1.73m (mL/min/1.73m).</items>
                </term_definitions>
                <term_definitions code="at0006">
                    <items id="text">Method</items>
                    <items id="description">The method of entering the Estimated Glomerular Filtration Rate.</items>
                </term_definitions>
                <term_definitions code="at0007">
                    <items id="text">Automatic entry</items>
                    <items id="description">Calculated and entered automatically by the clinical system.</items>
                </term_definitions>
                <term_definitions code="at0008">
                    <items id="text">Direct entry</items>
                    <items id="description">Calculated and entered directly by user.</items>
                </term_definitions>
                <term_definitions code="at0010">
                    <items id="text">Formula</items>
                    <items id="description">Formula used to calculate the Estimated Glomerular Filtration Rate.</items>
                </term_definitions>
                <term_definitions code="at0013">
                    <items id="text">MDRD formula - non-IDMS-normalized serum Cr</items>
                    <items id="description">Modification of Diet in Renal Disease study formula using standard creatinine measurement.</items>
                </term_definitions>
                <term_definitions code="at0014">
                    <items id="text">MDRD formula - IDMS-normalized serum Cr</items>
                    <items id="description">Modification of Diet in Renal Disease study formula using creatinine measured by an IDMS-calibrated assay.</items>
                </term_definitions>
                <term_definitions code="at0015">
                    <items id="text">CKD-EPI Creatinine</items>
                    <items id="description">Chronic Kidney Disease Epidemiology Collaboration formula using creatinine.</items>
                </term_definitions>
                <term_definitions code="at0016">
                    <items id="text">Cockcroft-Gault formula</items>
                    <items id="description">Estimated creatinine clearance rate (eCCr) using Cockcroft-Gault formula.</items>
                </term_definitions>
                <term_definitions code="at0017">
                    <items id="text">Bedside Schwartz equation</items>
                    <items id="description">Bedside IDMS-traceable Schwartz GFR Calculator for Children.</items>
                </term_definitions>
                <term_definitions code="at0018">
                    <items id="text">Comment</items>
                    <items id="description">Additional narrative about the eGFR, not captured in other fields.</items>
                </term_definitions>
                <term_definitions code="at0020">
                    <items id="text">Absolute eGFR</items>
                    <items id="description">Estimation of glomerular filtration rate calculated using the relative eGFR, weight and height.</items>
                </term_definitions>
                <term_definitions code="at0021">
                    <items id="text">LM Revised</items>
                    <items id="description">Revised Lund-Malm Study equation.</items>
                </term_definitions>
                <term_definitions code="at0022">
