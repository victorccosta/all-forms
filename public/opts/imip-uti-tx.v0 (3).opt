                    <items id="description">The amount of fluid loss by evaporation from the skin and respiratory tract.</items>
                    <items id="comment">The insensible loss is calculated using a range of parameters, including: body weight, body temperature, time in open surgery and use of a mechanical ventilator.</items>
                </term_definitions>
                <term_definitions code="at0005">
                    <items id="text">Total input</items>
                    <items id="description">The total amount of fluid administered or ingested during a specified interval of time.</items>
                </term_definitions>
                <term_definitions code="at0006">
                    <items id="text">Total output</items>
                    <items id="description">The total amount of fluid lost or excreted during a specified interval of time.</items>
                </term_definitions>
                <term_definitions code="at0007">
                    <items id="text">Fluid balance</items>
                    <items id="description">The difference between fluid input and output during a specified interval of time.</items>
                    <items id="comment">Fluid Balance = Total Fluid Input - (Total Fluid Output + Insensible Loss).</items>
                </term_definitions>
                <term_definitions code="at0008">
                    <items id="text">24 hour total</items>
                    <items id="description">Total fluid volume recorded during an interval of 24 hour duration.</items>
                </term_definitions>
